<div class="board">    
  <img src="assets/img/load.gif" id="loading" class="loading" />
  <table id="{{{stats_name}}}" class="tdisplay responsive table-hover table-condensed">
    <thead>
      <tr class="tr_header">
        <th id="PlayerRank" class="small-4">Rank</th>
        <th id="PlayerName" class="small-5 no-sort">Player Name</th>
        <th id="PlayerKills" class="small-5">Player Kills</th>
        <th id="PlayerKD" class="small-5">K/D</th>
        <th id="PlayerAI" class="small-5">AI Kills</th>
        <th id="PlayerDeath" class="small-5">Deaths</th>
        <th id="PlayerRevive" class="small-5">Revives</th>
        <th id="PlayerCapture" class="small-5">Captures</th>
        <th id="PlayerUpdate" class="small-5 no-sort">Last Update</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>